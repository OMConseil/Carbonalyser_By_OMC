{
  "manifest_version": 2,
  "name": "Carbonalyser by OMConseil",
  "description": "Analyse Internet usage carbon footprint, forked from TheShiftProject",
  "author": "OMConseil",
  "version": "0.0.0.3",
  "icons": {
    "48": "icons/icon-on-48.png"
  },
  "permissions": [
    "webRequest",
    "<all_urls>",
    "tabs",
    "idle"
  ],
  "background": {
    "scripts": [
      "script.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "icons/icon-off-48.png"
    }
  },
  "default_locale": "en"
}
